{
	"manifest_version":2,
	"name":"Reopen Tabs",
	"description":"reopen closed tabs",
	"version":"1.3",
	"background": {
		"scripts":	["background.js"],
		"persistent": false
	},
	"permissions":[
		"history","tabs"
	],
	"browser_action":{
		"default_icon"	: "icon.png",
		"default_title"	: "reopenTabs"
	}
}
